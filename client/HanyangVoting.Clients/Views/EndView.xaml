<UserControl x:Class="HanyangVoting.Clients.Views.EndView"
                     xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
             xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
             xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006" 
             xmlns:d="http://schemas.microsoft.com/expression/blend/2008" 
             xmlns:p="http://www.codeplex.com/prism"
             mc:Ignorable="d" FontFamily="서울남산체 EB" 
             d:DesignHeight="600" d:DesignWidth="1024">
    <DockPanel>
        <Grid DockPanel.Dock="Top" HorizontalAlignment="Stretch" Background="#7FFFFFFF">
            <StackPanel Margin="13">
                <TextBlock HorizontalAlignment="Center" FontSize="55" FontFamily="서울남산체 EB" FontWeight="Normal" Text="기표 완료" />
            </StackPanel>
        </Grid>
        <Grid>
            <StackPanel VerticalAlignment="Center" HorizontalAlignment="Center">
                <TextBlock FontSize="30" TextAlignment="Center">모든 투표에 대한 기표를 완료했습니다.</TextBlock>
                <TextBlock FontSize="30" Margin="0,30,0,0" TextAlignment="Center">나가기 전에 반드시 투표 티켓을 반납하십시오.</TextBlock>
            </StackPanel>
        </Grid>
    </DockPanel>
</UserControl>
